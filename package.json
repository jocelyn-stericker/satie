{
    "name": "satie",
    "version": "0.0.4",
    "description": "A sheet music renderer for the web",

    "author": "Josh Netterfield",
    "license": "AGPL-3.0",
    "homepage": "http://ripieno.github.io/satie",

    "repository": {
        "type": "git",
